{
  "name": "vvv-tt",
  "version": "0.1.0",
  "private": false,
  "description": "Starter template for vue2 vuetify vite typescript tailwind",
  "author": "rkanik.me@gmail.com",
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "lint": "eslint --fix --ignore-path .gitignore",
    "build:prod": "vite build --mode prod",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@vue/composition-api": "^1.4.9",
    "axios": "^0.26.1",
    "core-js": "^3.6.5",
    "moment": "^2.29.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-meta": "^2.4.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuelpers": "^2.3.2",
    "vuetify": "^2.6.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.4.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.50.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.1.5",
    "vite": "^2.8.6",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-vue2": "^1.9.3",
    "vue-cli-plugin-vuetify": "~2.4.8",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "template",
    "javascript",
    "typescript",
    "vuetify",
    "vue 2.x",
    "vite",
    "tailwind",
    "css"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/**.{js,json,vue,ts}": [
      "eslint --fix --ignore-path .gitignore",
      "pretty-quick --staged --ignore-path .gitignore"
    ]
  }
}